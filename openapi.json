        "summary": "Get Anime Info",
        "description": "Get detailed information about an anime\n\n- **identifier**: The anime identifier from search results",
        "operationId": "get_anime_info_anime__identifier__get",
        "parameters": [
          {
            "name": "identifier",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "description": "Anime identifier",
              "title": "Identifier"
            },
            "description": "Anime identifier"
          }
        ],
        "responses": {
